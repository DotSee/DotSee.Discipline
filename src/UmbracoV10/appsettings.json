{
  "$schema": "./appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "dea59239-3c84-4db3-b427-b2e83e6ff719",
        "SanitizeTinyMce": true
      },
      "Content": {
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        }
      }
    }
  },
  "ConnectionStrings": {
    "umbracoDbDSN": "Data Source=|DataDirectory|/Umbraco.sqlite.db;Cache=Shared;Foreign Keys=True;Pooling=True",
    "umbracoDbDSN_ProviderName": "Microsoft.Data.SQLite"
  },
  "DotSee.Discipline": {
    "AutoNode": {
      "Settings": {
        "LogLevel": "Verbose",
        "RepublishExistingNodes": "false"
      },
      "Rules": [
        {
          "CreatedDocTypeAlias": "sampleBasePage",
          "DocTypeAliasToCreate": "contentPage",
          "NodeName": "MyAutoGeneratedPage",
          "BringNewNodeFirst": "true",
          "OnlyCreateIfNoChildren": "false",
          "CreateIfExistsWithDifferentName": "false",
          "DictionaryItemForName": "",
          "Blueprint": ""
        },
        {
          "CreatedDocTypeAlias": "sampleBaseContainer1",
          "DocTypeAliasToCreate": "sampleBasePage",
          "NodeName": "MyAutoGeneratedPage3",
          "BringNewNodeFirst": "true",
          "OnlyCreateIfNoChildren": "false",
          "CreateIfExistsWithDifferentName": "false",
          "DictionaryItemForName": "test",
          "Blueprint": "test"
        },
        {
          "CreatedDocTypeAlias": "sampleBasePage",
          "DocTypeAliasToCreate": "contentPageInvariant",
          "NodeName": "InvariantAutoGeneratedPage",
          "BringNewNodeFirst": "true",
          "OnlyCreateIfNoChildren": "false",
          "CreateIfExistsWithDifferentName": "false",
          "DictionaryItemForName": "",
          "Blueprint": ""
        },
        {
          "CreatedDocTypeAlias": "sampleBasePage",
          "DocTypeAliasToCreate": "contentPage",
          "NodeName": "VariantAutoGeneratedPage",
          "BringNewNodeFirst": "false",
          "OnlyCreateIfNoChildren": "false",
          "CreateIfExistsWithDifferentName": "false",
          "DictionaryItemForName": "",
          "Blueprint": ""
        }
      ]
    },
    "NodeRestrict": {
      "Settings": {
        "PropertyAlias": "numberofsubnodes",
        "ShowWarnings": true
      },
      "Rules": [
        {
          "ParentDocType": "sampleBasePage",
          "ChildDocType": "contentPage",
          "MaxNodes": 4,
          "ShowWarnings": true,
          "CustomMessage": "",
          "CustomMessageCategory": "",
          "CustomWarningMessage": "",
          "CustomWarningMessageCategory": ""
        }//,
        //{
        //  "ParentDocType": "nodeRestrictVariantRoot",
        //  "ChildDocType": "nodeRestrictVariantChild",
        //  "MaxNodes": 1,
        //  "ShowWarnings": true,
        //  "CustomMessage": "",
        //  "CustomMessageCategory": "",
        //  "CustomWarningMessage": "",
        //  "CustomWarningMessageCategory": ""
        //},
        //{
        //  "ParentDocType": "nodeRestrictInvariantRoot",
        //  "ChildDocType": "nodeRestrictInvariantChild",
        //  "MaxNodes": 1,
        //  "ShowWarnings": true,
        //  "CustomMessage": "",
        //  "CustomMessageCategory": "",
        //  "CustomWarningMessage": "",
        //  "CustomWarningMessageCategory": ""
        //}
      ]
    },
    "VirtualNodes": {
      "Rules": [ "sampleBasePage" ]
    },
    "VariantsHider": {
      "Enabled": true,
      "Caption": "Test Caption"
    },
    "NodeProtect": {
      "Settings": {
        "PropertyAlias": "donotdelete"
      },
      "Rules": [
        {
          "DocTypeAlias": "",
          "DocumentGuids": "1d1529e8-3199-403f-bf3e-965bbb945d01",
          "IgnoreForAdmins": false,
          "CustomMessage": "fsfdsa",
          "CustomMessageCategory": "sdfsadfsa"
        }
      ]
    }
  }
 }